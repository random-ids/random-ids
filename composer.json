  "keywords": [
    "hashids",
    "hashid",
    "hash",
    "ids",
    "random",
    "unique",
    "id"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Will Lin",
      "email": "will@cn09.com"
    }
  ],
  "support": {
    "email": "will@cn09.com",
    "issues": "https://github.com/random-ids/random-ids/issues"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "RandomIds\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RandomIds\\Tests\\": "tests/"
    }
  }
}
